{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./my_project/frontend/static/dist",
    "declaration": true,
    "sourceMap": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "lib": ["ES6", "DOM"]
  },
  "include": ["my_project/**/ts/*"],
  "exclude": ["node_modules"]
}
